  },
  "optionalDependencies": {
    "nunjucks": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elcharitas/knuckjs.git"
  },
  "keywords": [
    "nunjucks",
    "express",
    "browser",
    "response",
    "knuckjs"
  ],
  "bugs": {
    "url": "https://github.com/elcharitas/knuckjs/issues"
